{
  "name": "students-ost-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev-api\" \"npm run dev-app\"",
    "dev-app": "cd ./app/ && vite",
    "dev-api": "cd ./api/ && nodemon server.ts",
    "build": "npm run build-api && npm run build-app",
    "build-app": "cd ./app/ && vue-tsc && vite build",
    "build-api": "cd ./api/ && tsc",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.9.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/pg": "^8.6.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^4.1.1",
    "vue-tsc": "^1.0.9"
  }
}
